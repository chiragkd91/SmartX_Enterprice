{
  "name": "coma-app",
  "version": "1.0.0",
  "description": "AI-powered code editor with advanced features including semantic search, real-time collaboration, and intelligent code assistance",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "start": "node dist/main.js",
    "docker:build": "docker build -t coma-app .",
    "docker:run": "docker run -p 3000:3000 coma-app",
    "setup": "npm install && npm run build"
  },
  "keywords": [
    "ai",
    "code-editor",
    "artificial-intelligence",
    "vscode",
    "semantic-search",
    "collaboration",
    "llm",
    "typescript",
    "react"
  ],
  "author": "Coma-App Team",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "@monaco-editor/react": "^4.6.0",
    "monaco-editor": "^0.44.0",
    "@anthropic-ai/sdk": "^0.17.1",
    "openai": "^4.28.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "express": "^4.18.2",
    "fastify": "^4.24.3",
    "ws": "^8.14.2",
    "redis": "^4.6.10",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6",
    "@pinecone-database/pinecone": "^1.1.2",
    "langchain": "^0.1.25",
    "@langchain/openai": "^0.0.14",
    "@langchain/anthropic": "^0.1.11",
    "uuid": "^9.0.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4",
    "axios": "^1.6.2",
    "lodash": "^4.17.21",
    "date-fns": "^2.30.0",
    "react-router-dom": "^6.20.1",
    "@tanstack/react-query": "^5.8.4",
    "zustand": "^4.4.7",
    "framer-motion": "^10.16.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "lucide-react": "^0.294.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/node": "^20.9.4",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.9",
    "@types/pg": "^8.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.0",
    "vite": "^5.0.5",
    "vitest": "^0.34.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@playwright/test": "^1.40.1",
    "eslint": "^8.54.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "dotenv": "^16.3.1",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/coma-app.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/coma-app/issues"
  },
  "homepage": "https://github.com/your-org/coma-app#readme"
